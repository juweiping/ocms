<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!-- 
	$Id: cms-module-schedule-orm-oracle.xml 3939 2010-10-27 08:41:36Z orangeforjava $
-->
<beans>
	<bean id="scheduleOracleMapping"
		class="org.openuap.runtime.setup.persistence.hibernate.MappingResources">
		<property name="dialect">
			<value>org.hibernate.dialect.Oracle9Dialect</value>
		</property>
		<property name="mappings">
			<list>
				<value>org/openuap/cms/schedule/model/oracle/Schedule.hbm.xml</value>			
			</list>
		</property>
	</bean>
</beans>