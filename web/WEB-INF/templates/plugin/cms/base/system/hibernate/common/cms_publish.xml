<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<!-- Generated by Weiping Ju,this is a publish table template.  -->
<hibernate-mapping>
  <#if ct.tableName?default("")!="">
  	<class entity-name="${ct.entityName}Publish" table="${ct.tableName}_publish">
  <#else>
  	<class entity-name="Publish_${ct.tableId}" table="cms_publish_${ct.tableId}">
  </#if>
    <!--the fixed column-->
    <id name="indexId" type="long">
      <column name="IndexID"  not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="contentId" type="long" >
      <column name="ContentID"  not-null="true" default="'0'"/>
    </property>
    <property name="nodeId"  type="long" >
      <column name="NodeID"  not-null="true" default="'0'"/>
    </property>
    <property name="tableId"  type="long" >
      <column name="tableId"  default="'0'"/>
    </property>
    <property name="publishDate"  type="long">
      <column name="PublishDate" />
    </property>
    
    <property name="creationDate" type="long" >
      <column name="CreationDate" not-null="true" default="'0'"/>
    </property>
    <property name="modifiedDate" type="long" >
      <column name="ModifiedDate"  not-null="true" default="'0'"/>
    </property>
    <property name="creationUserId" type="long" >
      <column name="CreationUserID"  not-null="true" default="'0'"/>
    </property>
	<property name="creationUserName" type="string">
      <column name="CreationUserName"   />
    </property>
    <property name="lastModifiedUserId" type="long" >
      <column name="LastModifiedUserID"  not-null="true" default="'0'"/>
    </property>
	<property name="lastModifiedUserName" type="string" >
      <column name="LastModifiedUserName" default="'no'" />
    </property>
    <property name="contributionUserId" type="long" >
      <column name="ContributionUserID"  not-null="true" default="'0'"/>
    </property>
	 <property name="contributionUserName" type="string">
      <column name="ContributionUserName"   />
    </property>
    <property name="contributionId" type="long" >
      <column name="ContributionID"  not-null="true" default="'0'"/>
    </property>
    <property name="url" column="URL"  type="string" length="250"/>
   
    <property name="top" type="integer"  >
      <column name="Top2" default="'0'" not-null="true"/>
    </property>
    <property name="pink"  type="integer" >
      <column name="Pink" default="'0'" not-null="true" />
    </property>
    <property name="sort" type="integer"  >
      <column name="Sort" default="'0'" not-null="true" />
    </property>
    <property name="nodeUrl" type="string" >
      <column name="NodeURL" default="'no'" not-null="true"/>
    </property>
     <property name="nodeName" type="string" >
      <column name="NodeName" default="'no'" not-null="true"/>
    </property>
    <!--the variable column-->
    <#if ct.contentFieldsSet??>
    <#list ct.contentFieldsSet as field>
    <#if field.fieldType?default("")=="contentlink">
     <property name="${field.fieldName}" type="text">
       <column name="${field.fieldName}" />
     </property>
    <#elseif field.fieldType?default("")=="varchar">

        <#if field.fieldSize?default("")=="">
          <property name="${field.fieldName}" column="${field.fieldName}" type="string"/>
        <#else>
          <property name="${field.fieldName}" column="${field.fieldName}" type="string" length="${field.fieldSize}"/>
        </#if>
    <#else>
      <property name="${field.fieldName}" type="${field.fieldType}">
        <#if field.fieldSize?default("")=="">
          <column name="${field.fieldName}" />
          <#else>
            <column name="${field.fieldName}" length="${field.fieldSize}" />
        </#if>
      </property>
    </#if>
    </#list>
    </#if>
  </class>
</hibernate-mapping>
